<% content_for :header do %>
  <p id="code" style="display:none"><%= session[:company_id]%></p>
  <section class="content-header">
    <div class="row">
      <div class="col-xs-12">
        <ol class="breadcrumb" style="font-size:medium;">
          <li><a href="/dashboard"><i class="fa fa-dashboard"></i> 首页 </a></li>
          <li class="active">施工合同</li>
        </ol>
      </div>
    </div>
  </section>
<% end %>

<% content_for :content do %>
  <section class="content" style="margin-top:-25px;" >
    <div class="col-md-12">
      <div class="row">
        <div class="box">
          <div class="box-title">
            <div style="margin: 0 auto;width: 200px;"><h4 style="margin:0 0;padding-top:30px;width: 200px;text-align: center">施工合同统计表</h4></div>
          </div>
          <div class="box-body">
            <table class="table table-striped table-hover" id="tTrolleys">
              <thead>
                <tr style="border-bottom:1px solid gray;">
                  <th>合同编号</th>
                  <th>甲方</th>
                  <th>乙方</th>
                  <th>金额(元)</th>
                  <th>签订日期</th>
                </tr>
              </thead>
            </table>
            <p>合计资金(元)：90000</p>
          </div>
        </div>
      </div>
    </div>
  </section>
  
  

<% end %>