<% content_for :header do %>
  <p id="code" style="display:none"><%= session[:company_id]%></p>
  <section class="content-header">
    <div class="row">
      <div class="col-xs-12">
        <ol class="breadcrumb" style="font-size:medium;">
          <li><a href="/dashboard"><i class="fa fa-dashboard"></i> 首页 </a></li>
          <li class="active">市级审核</li>
        </ol>
      </div>
    </div>
  </section>
<% end %>

<% content_for :content do %>
  <section class="content" style="margin-top:-25px;" >
    <div class="row">
      <div class="col-md-12">
        <div class="box">
          <div class="box-header with-border">
            <h5>核销申请列表</h5>
          </div>
          <div class="box-body">
            <table class="table table-striped table-hover" id="tTrolleys">
              <thead>
                <tr style="border-bottom:1px solid gray;">
                  <th>项目名称</th>
                  <th>项目编号</th>
                  <th>项目地址</th>
                  <th>核销原因</th>
                  <th>停用时间</th>
                  <th>项目实景照片</th>
                  <th>所在乡镇意见</th>
                  <th></th>
                </tr>
              </thead>
            </table>
          </div>
        </div>
      </div>
    </div>
  </section>
<% end %>